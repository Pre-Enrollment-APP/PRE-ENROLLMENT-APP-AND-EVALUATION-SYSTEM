<?xml version="1.0" encoding="utf-8"?>

<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".Pre_Enrollment"
    android:background="@drawable/bglight"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">


        <View
            android:layout_width="match_parent"
            android:layout_height="40dp"
            android:background="@color/blue"></View>

        <TextView
            android:id="@+id/header_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/arial"
            android:padding="7dp"
            android:text="Pre-enlistment"
            android:textColor="@color/white"
            android:textSize="17dp" />

        <ImageButton
            android:id="@+id/back"
            android:layout_width="23dp"
            android:layout_height="22dp"
            android:layout_marginStart="1dp"
            android:layout_marginTop="8dp"
            android:background="?attr/selectableItemBackground"
            android:paddingStart="5dp"
            android:paddingTop="3dp"
            android:paddingBottom="3dp"
            android:scaleType="fitXY"
            android:src="@drawable/back"

            />


        <ImageView
            android:id="@+id/ccc"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="50dp"
            android:src="@drawable/ccc" />


        <TextView
            android:id="@+id/gmail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:layout_toRightOf="@id/ccc"
            android:hint="Student number"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/birthday"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="80dp"
            android:layout_toRightOf="@id/ccc"
            android:hint="Birthday"
            android:visibility="invisible" />

        <TextView
            android:id="@+id/cdis"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ccc"
            android:layout_centerHorizontal="true"
            android:fontFamily="@font/arial"
            android:gravity="center"
            android:lineSpacingExtra="5dp"
            android:text="COLLEGE DEPARTMENT \n INFORMATION SHEET"
            android:textColor="@color/black"
            android:textSize="12dp" />

        <TextView
            android:id="@+id/txt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/cdis"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/arial"
            android:gravity="center"
            android:lineSpacingExtra="5dp"
            android:text="Please fill out the form"
            android:textSize="10dp" />

        <TextView
            android:id="@+id/studentName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="5dp"
            android:fontFamily="@font/arial"
            android:hint="Name"
            android:textColor="@color/black"
            android:textSize="20dp" />


        <TextView
            android:id="@id/course"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/studentName"
            android:layout_centerHorizontal="true"
            android:layout_marginStart="8dp"
            android:clickable="false"
            android:cursorVisible="false"
            android:focusable="false"
            android:focusableInTouchMode="false"
            android:fontFamily="@font/arial"
            android:hint="this is the course"
            android:paddingStart="10dp"
            android:textAlignment="center"
            android:textColor="@color/gray"
            android:textSize="15dp" />

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/course"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="10dp"
            android:fadeScrollbars="false"
            android:scrollbarFadeDuration="0"
            android:scrollbars="vertical">

            <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="0dp">

                <androidx.cardview.widget.CardView
                    android:id="@+id/card"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="15dp"

                    android:layout_marginEnd="15dp"
                    android:backgroundTint="@color/superwhite"
                    app:cardCornerRadius="10dp"
                    app:cardElevation="12dp">

                    <androidx.constraintlayout.widget.ConstraintLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:paddingTop="5dp"
                        android:paddingBottom="15dp">

                        <TextView
                            android:id="@+id/address"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="80dp"
                            android:hint="Address"
                            android:visibility="invisible"
                            tools:ignore="MissingConstraints" />

                        <TextView
                            android:id="@+id/schoolyeartext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@+id/semester"
                            android:layout_marginStart="20dp"
                            android:layout_marginTop="18dp"
                            android:layout_toRightOf="@id/section"
                            android:fontFamily="@font/arial"
                            android:text="S.Y"
                            android:textColor="@color/black"
                            android:textSize="15dp"
                            tools:ignore="MissingConstraints" />

                        <EditText
                            android:id="@+id/schoolyear"
                            android:layout_width="120dp"
                            android:layout_height="25dp"
                            tools:ignore="MissingConstraints"
                            android:layout_below="@+id/semester"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="3dp"
                            android:layout_marginTop="15dp"
                            android:layout_toRightOf="@+id/schoolyeartext"
                            android:background="@drawable/editext_miniround"
                            android:fontFamily="@font/arial"
                            android:hint="2000-2001"
                            android:paddingLeft="5dp"
                            android:textColor="@color/black"
                            android:textSize="13dp" />

                        <TextView
                            android:id="@+id/sectiontext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/semester"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="18dp"
                            android:fontFamily="@font/arial"
                            tools:ignore="MissingConstraints"
                            android:paddingLeft="20dp"
                            android:text="Section"
                            android:textColor="@color/black"
                            android:textSize="15dp" />

                        <Spinner
                            android:id="@+id/section"
                            android:layout_width="80dp"
                            android:layout_height="22dp"
                            android:layout_below="@id/semester"
                            android:layout_marginStart="10dp"
                            android:layout_marginLeft="5dp"
                            tools:ignore="MissingConstraints"
                            android:layout_marginTop="15dp"
                            android:layout_toRightOf="@+id/sectiontext"
                            android:background="@drawable/editext_miniround"
                            android:paddingLeft="5dp" />


                        <TextView
                            android:id="@+id/moptext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/section"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="15dp"
                            android:fontFamily="@font/arial"
                            android:paddingLeft="20dp"
                            tools:ignore="MissingConstraints"
                            android:text="Mode of Payment"
                            android:textColor="@color/black"
                            android:textSize="15dp" />

                        <Spinner
                            android:id="@+id/mop"
                            android:layout_width="185dp"
                            android:layout_height="22dp"
                            android:layout_below="@+id/section"
                            android:layout_marginLeft="10dp"
                            tools:ignore="MissingConstraints"
                            android:layout_marginTop="15dp"
                            android:layout_toRightOf="@+id/moptext"
                            android:background="@drawable/editext_miniround"
                            android:paddingLeft="5dp" />

                        <TextView
                            android:id="@+id/semestertext"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginStart="10dp"
                            android:layout_marginTop="17dp"
                            android:fontFamily="@font/arial"
                            tools:ignore="MissingConstraints"
                            android:paddingLeft="20dp"
                            android:text="Year &amp; Semester"
                            android:textColor="@color/black"
                            android:textSize="15dp" />


                        <Spinner
